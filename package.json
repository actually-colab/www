{
  "name": "www",
  "license": "GPL-2.0-only",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^2.4.0",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.2.2",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "rsuite": "^4.9.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"react-scripts start\"",
    "prebuild": "less-watch-compiler --config less-watcher.config.prod.json",
    "build": "react-scripts build",
    "postinstall": "husky install",
    "predeploy": "yarn build",
    "deploy": "netlify deploy --prod",
    "test": "react-scripts test",
    "format": "prettier --config .prettierrc.js --write \"**/*.{ts,js,tsx,css,html,md}\"",
    "lint": "eslint --config ./.eslintrc.js --ext .js,.ts,.jsx,.tsx src/",
    "validate": "npx tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.4",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "concurrently": "^6.0.2",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "less": "^4.1.1",
    "less-watch-compiler": "^1.14.6",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
